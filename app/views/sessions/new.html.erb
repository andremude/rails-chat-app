<div class="signin-form text-center d-flex justify-content-center">
  <%= form_for (:session) do |f| %>
    <div class="d-flex flex-column mt-3 w-5">
      <%= f.label :username, 'Username' %>
      <%= f.text_field :username, autocomplete: 'off' %>
      <%= f.submit 'Sign in' %>
    </div>
  <% end %>
</div>
